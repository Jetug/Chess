<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:system="clr-namespace:System;assembly=mscorlib">
   
    <Color x:Key="WhiteCellColor">#FFD8DCB8</Color>
    <SolidColorBrush x:Key="WhiteCellBrush" Color="{StaticResource WhiteCellColor}"/>

    <Color x:Key="BlackCellColor">#FF4C2312</Color>
    <SolidColorBrush x:Key="BlackCellBrush" Color="{StaticResource BlackCellColor}"/>

    <Color x:Key="FieldBorder">#FF000000</Color>
    <SolidColorBrush x:Key="FieldBorderBrush" Color="{StaticResource FieldBorder}"/>

    <Color x:Key="ButtonColor">#FF3232A6</Color>
    <SolidColorBrush x:Key="ButtonBrush" Color="{StaticResource ButtonColor}"/>

    <Image x:Key="FlipIcon" Source="pack://application:,,,/Styles/FlipIcon.png"/>

    <Color x:Key="MainWindowColor">#FF2B2B2B</Color>
    <SolidColorBrush x:Key="MainWindowBrush" Color="{StaticResource MainWindowColor}"/>

    <Color x:Key="ForegroundColor">#FF000000</Color>
    <SolidColorBrush x:Key="ForegroundBrush" Color="{StaticResource ForegroundColor}"/>

</ResourceDictionary>